                        <TextField fx:id="textfield1change" prefWidth="300" prefHeight="25" visible="false" editable="false" style="-fx-focus-color: #FF0000; -fx-faint-focus-color: transparent; -fx-highlight-fill: #FF0000; -fx-font-family: Times New Roman; -fx-font-style: italic; -fx-font-size: 12; -fx-text-fill: #000000"/>
                    </HBox>
                    <HBox id="HBox8">
                        <Label fx:id="label4change" prefWidth="200" prefHeight="25" visible="false" style="-fx-font-family: Times New Roman; -fx-font-size: 12; -fx-text-fill: #000000"/>
                        <TextField fx:id="textfield2change" prefWidth="300" prefHeight="25" visible="false" editable="false" style="-fx-focus-color: #FF0000; -fx-faint-focus-color: transparent; -fx-highlight-fill: #FF0000; -fx-font-family: Times New Roman; -fx-font-style: italic; -fx-font-size: 12; -fx-text-fill: #000000"/>
                    </HBox>
                    <StackPane id="StackPane2">
                        <VBox id="VBox6">
                            <HBox id="HBox9">
                                <Label fx:id="label5change" prefWidth="200" prefHeight="25" visible="false" style="-fx-font-family: Times New Roman; -fx-font-size: 12; -fx-text-fill: #000000"/>
                                <TextField fx:id="textfield3change" prefWidth="300" prefHeight="25" visible="false" editable="false" style="-fx-focus-color: #FF0000; -fx-faint-focus-color: transparent; -fx-highlight-fill: #FF0000; -fx-font-family: Times New Roman; -fx-font-style: italic; -fx-font-size: 12; -fx-text-fill: #000000"/>
                            </HBox>
                            <HBox id="HBox10">
                                <Label fx:id="label6change" prefWidth="200" prefHeight="25" visible="false" style="-fx-font-family: Times New Roman; -fx-font-size: 12; -fx-text-fill: #000000"/>
                                <TextField fx:id="textfield4change" prefWidth="300" prefHeight="25" visible="false" editable="false" style="-fx-focus-color: #FF0000; -fx-faint-focus-color: transparent; -fx-highlight-fill: #FF0000; -fx-font-family: Times New Roman; -fx-font-style: italic; -fx-font-size: 12; -fx-text-fill: #000000"/>
                            </HBox>
                        </VBox>
                    </StackPane>
                </VBox>
            </StackPane>
            <HBox id="HBox11" alignment="CENTER_RIGHT">
                <Button fx:id="button9add" text="Add" prefWidth="75" prefHeight="15" visible="false" style="-fx-background-color: #4B0000; -fx-font-family: Times New Roman; -fx-font-style: italic; -fx-font-size: 14; -fx-text-fill: #FFFFFF" onAction="#button9add"/>
                <Button fx:id="button10edit" text="Edit" prefWidth="75" prefHeight="15" visible="false" style="-fx-background-color: #4B0000; -fx-font-family: Times New Roman; -fx-font-style: italic; -fx-font-size: 14; -fx-text-fill: #FFFFFF" onAction="#button10edit"/>
            </HBox>
        </VBox>
    </HBox>
    <TableView fx:id="tableview1audiolibrary" prefWidth="990" prefHeight="150" maxWidth="990" style="-fx-focus-color: #FF0000; -fx-faint-focus-color: transparent; -fx-font-family: Times New Roman; -fx-font-size: 12; -fx-text-fill: #000000" stylesheets="/TableView.css"/>
    <HBox id="HBox12">
        <TextField fx:id="textfield6search" prefWidth="150" prefHeight="25" style="-fx-focus-color: #FF0000; -fx-faint-focus-color: transparent; -fx-highlight-fill: #FF0000; -fx-font-family: Times New Roman; -fx-font-style: italic; -fx-font-size: 12; -fx-text-fill: #000000"/>
        <HBox id="HBox13" alignment="CENTER_RIGHT" prefWidth="840">
            <Label fx:id="label7numberofartistssongs" style="-fx-font-family: Times New Roman; -fx-font-size: 14; -fx-text-fill: #000000"/>
        </HBox>
    </HBox>
</FlowPane>